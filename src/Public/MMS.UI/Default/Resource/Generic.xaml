        <ResourceDictionary Source="/MMS.UI;Component/Window/Default/DefaultWindow.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style TargetType="{x:Type local:DefaultWindow}" BasedOn="{StaticResource DefaultWindowStyle}"/>
</ResourceDictionary>